<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>

<VBox alignment="TOP_RIGHT" prefHeight="445.0" prefWidth="154.0" styleClass="toolbar-container" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainScreenMenuController">
   <children>
      <Button fx:id="orderVisit" mnemonicParsing="false" onMouseClicked="#loadOrderVisit" prefHeight="80.0" prefWidth="160.0" styleClass="toolbar-button" text="Order A Visit">
         <!-- Icon can be set here if you have an ImageView setup -->
      </Button>
      <Button fx:id="travelerLogin" mnemonicParsing="false" onMouseClicked="#loadTravelerLogin" prefHeight="80.0" prefWidth="160.0" styleClass="toolbar-button" text="Traveler Login">
         <!-- Icon can be set here if you have an ImageView setup -->
      </Button>
      <Button fx:id="memberLogin" mnemonicParsing="false" onMouseClicked="#loadMemberLogin" prefHeight="80.0" prefWidth="160.0" styleClass="toolbar-button" text="Member Login">
         <!-- Icon can be set here if you have an ImageView setup -->
      </Button>
      <Button fx:id="showPrices" mnemonicParsing="false" onMouseClicked="#loadPrices" prefHeight="80.0" prefWidth="160.0" styleClass="toolbar-button" text="Prices">
         <!-- Icon can be set here if you have an ImageView setup -->
      </Button>
      <Button fx:id="contactUs" mnemonicParsing="false" onMouseClicked="#loadContactUs" prefHeight="80.0" prefWidth="160.0" styleClass="toolbar-button" text="Contact Us">
         <!-- Icon can be set here if you have an ImageView setup -->
      </Button>
   </children>
</VBox>
